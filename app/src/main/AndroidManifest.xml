<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="joe.com.learnelectricalhacking">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FirstPartSummaryActivity"></activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="app-id" />

        <activity android:name=".MainActivity" />
        <activity android:name=".DoneActivity" />
        <activity android:name=".ResistorsVoltageAndCurrentActivity" />
        <activity android:name=".ConnectingActivity" />
        <activity android:name=".AllDoneActivity" />
        <activity android:name=".OtherCapacitorsActivity" />
        <activity android:name=".InductorActivity" />
        <activity android:name=".ResistorsActivity" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>